# Application Configuration
APP_DOMAIN=http://localhost:5001

# SMTP Configuration for Email
SMTP_SERVER=smtp.example.com
SMTP_PORT=587
SMTP_USER=user@example.com
SMTP_PASSWORD=yourpassword
SMTP_FROM=noreply@example.com

# Admin Account
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=Admin123!

# LLM settings
DEFAULT_LLM_MODEL=gpt-4o
DEFAULT_LLM_TEMPERATURE=0.2
DEFAULT_LLM_MAX_TOKENS=4000

# API Keys (needed for AI feedback) - uncomment only the ones you're using
# OpenAI
# OPENAI_API_KEY=your-openai-api-key-here

# Anthropic 
# ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Google
# GOOGLE_API_KEY=your-google-api-key-here

# Groq
# GROQ_API_KEY=your-groq-api-key-here

# Together AI
# TOGETHERAI_API_KEY=your-togetherai-api-key-here

# Hugging Face
# HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# Ollama (local models)
# OLLAMA_API_BASE=http://localhost:11434

# LiteLLM Configuration (used for model routing)
LITELLM_CACHE=True
LITELLM_CACHE_TIMEOUT=3600
# LITELLM_PROVIDER=openai  # Uncomment to force a specific provider